{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "MONGODB_URI": "mongodb://localhost:27017/Necleo"
    }
  }
  