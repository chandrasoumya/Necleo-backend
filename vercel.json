{
    "version": 2,
    "builds": [
        {
            "src": ".index.js",
            "dest": "@vercel/node"
          }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
        "MONGODB_URI": "mongodb://localhost:27017/Necleo"
      }
  }
  